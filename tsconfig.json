{
  "compilerOptions": {
    "module": "es2015",
    "target": "es2015",
    "noImplicitAny": false,
    "sourceMap": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "types": ["node"],
    "importHelpers": true,
    "noEmitHelpers": true
  },
  "exclude": ["node_modules"]
}
